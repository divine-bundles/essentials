(nf) README.adoc

#
# Copy runcoms without queueing
#
(prefix: runcoms)
(rno) [^/]+.(ba|z)?sh

#
# First task: blanks
#
(prefix: blanks) (flags: d)
(+o)  .hushlogin
      .pre.bash
      .pre.zsh
      .pre.sh
      .post.bash
      .post.zsh
      .post.sh

#
# Second task: base runcoms
#
(queue: split) (prefix: configs) (flags: d)
.bash_profile
.bashrc
.zprofile
.zshrc